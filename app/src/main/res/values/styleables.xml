<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DragRightFrameLayout">
    </declare-styleable>
    <declare-styleable name="MenuLayout">
        <attr name="android:layout_gravity"/>
    </declare-styleable>
    <declare-styleable name="DragMenuItemView">
        <attr name="selectDrawable" format="reference"/>
        <attr name="unSelectDrawable" format="reference"/>
        <attr name="textColor" format="color"/>
        <attr name="textSelectColor" format="color"/>
        <attr name="selectBackground" format="reference"/>
        <attr name="unSelectBackground" format="reference"/>
        <attr name="title" format="string"/>
        <attr name="android:textSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="android:src" format="reference"/>
    </declare-styleable>
    <declare-styleable name="PopMenuButton">
        <attr name="src_open" format="reference"/>
        <attr name="src_default" format="reference"/>
        <attr name="color_default" format="reference"/>
        <attr name="color_open" format="reference"/>
    </declare-styleable>

    <declare-styleable name="TextViewColorPoint">
        <attr name="color_point" format="color"/>
    </declare-styleable>
    <declare-styleable name="Label">
        <attr name="left_enable" format="boolean"/>
        <attr name="right_enable" format="boolean"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text_color" format="color"/>
        <attr name="src_left" format="reference"/>
        <attr name="src_right" format="reference"/>
        <attr name="text" format="string"/>
        <attr name="text_padding" format="dimension"/>
    </declare-styleable>
</resources>